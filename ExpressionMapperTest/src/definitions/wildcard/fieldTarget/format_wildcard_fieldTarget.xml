<?xml version="1.0" encoding="UTF-8"?>
<format.xml>
	<fmtDef id="fieldTarget_oneLevelFieldFmt">
		<mapperConverterExpression>
			<map fromExpression="functs_BTTFieldTargetFunctions.sum(oneLevelKCollIColl$*$testBigInteger)" to="testField"/>
		</mapperConverterExpression>
	</fmtDef>	
	
	<fmtDef id="fieldTarget_oneLevelFieldCountFmt">
		<mapperConverterExpression>
			<map fromExpression="functs_BTTFieldTargetFunctions.countHigherThan(oneLevelKCollIColl$*$testBigInteger, 2000000)" to="testField"/>
		</mapperConverterExpression>
	</fmtDef>	
	
	<fmtDef id="fieldTarget_oneLevelFieldDateFmt">
		<mapperConverterExpression>
			<map fromExpression="functs_BTTFieldTargetFunctions.latestDate(oneLevelKCollIColl$*$testKColl2$testDate)" to="testField"/>
		</mapperConverterExpression>
	</fmtDef>	
</format.xml>
